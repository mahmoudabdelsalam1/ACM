#include<iostream>
#include<iomanip>
using namespace std;
 
 
int main()
{
	int a, b, c,d;
	cin >> a >> b >> c;
	if (a > b)
	{
		d = a;
		a = b;
		b = d;
	}
	if (b > c)
	{
		d = b;
		b = c;
		c = d;
 
	}
	if (a > b)
	{
		d = a;
		a = b;
		b = d;
	}
	cout << a << " " << b << " " << c << " ";
 
 
 
 
	return 0;
}
